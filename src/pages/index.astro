---
import IconList from "../components/IconList.astro";
import Layout from "../layout/Layout.astro";
---

<Layout title="ByteIcons">
  <header class="flex flex-col items-center">
    <h1 class="text-4xl subpixel-antialiased font-bold mb-2">ByteIcons</h1>
    <p class="text-base mb-4 font-semibold">
      Optimized SVG Icons of general tech related things.
    </p>
  </header>
  <main class="w-3/4">
    <h2 class="text-xl font-bold">All icons:</h2>
    <IconList />
  </main>
</Layout>
